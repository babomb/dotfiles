{
    "workbench.startupEditor": "newUntitledFile",
    "window.zoomLevel": -1,
    "editor.fontSize": 14,
    // "editor.fontFamily": "''Consolas','Meiryo UI'",
    "markdown.preview.fontFamily": "'Meiryo UI'",
    //    "workbench.colorTheme": "Hybrid Next",
    "files.eol": "\n",
