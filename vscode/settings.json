    "C_Cpp.errorSquiggles": "Enabled",
    "workbench.colorCustomizations": {
        "statusBar.background": "#021d24",
        "statusBar.noFolderBackground": "#bbbbbbec",
        "statusBar.debuggingBackground": "#17c200"
    },
    "workbench.sideBar.location": "right",
    "files.exclude": {
        "thirdparty": true,
        "exports": true,
        ".vscode": true,
        "ctags": true,
        "GTAGS": true,
        "GRTAGS": true,
        "GPATH": true,
        "**/*.o": true,
        //"**/*.s": true,
        "**/*.exe": true,
        "**/*.bak": true
    },
